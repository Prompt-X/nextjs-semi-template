{
  "name": "nextjs-semi-template",
  "version": "0.1.0",
  "description": "A Next.js template with TypeScript, Tailwind CSS, ESLint, Prettier, Husky, and Lint-staged",
  "author": "Prompt technology",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky",
    "format": "prettier --write .",
    "quick": "pretty-quick --staged"
  },
  "dependencies": {
    "@douyinfe/semi-icons": "^2.63.0",
    "@douyinfe/semi-ui": "^2.63.0",
    "@radix-ui/react-icons": "^1.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.414.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@douyinfe/semi-next": "^2.62.1",
    "@douyinfe/semi-webpack-plugin": "^2.62.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwind-children": "^0.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.1",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
